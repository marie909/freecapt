# HeyGen API Configuration
# Get your API key from: https://app.heygen.com/settings?nav=API
HEYGEN_API_KEY=your_heygen_api_key_here

# Optional: Base API URL (defaults to https://api.heygen.com)
# HEYGEN_BASE_URL=https://api.heygen.com

# Avatar Configuration
# Default Avatar ID (from labs.heygen.com/interactive-avatar)
NEXT_PUBLIC_AVATAR_ID=your_avatar_id_here

# Default Voice ID
NEXT_PUBLIC_VOICE_ID=your_voice_id_here

# Knowledge Base ID (from labs.heygen.com)
NEXT_PUBLIC_KNOWLEDGE_BASE_ID=your_knowledge_base_id_here
